<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">Library</li>
  </ol>
</nav>


<form class="row g3 needs-validation" action="/" method="get">
{{#each rows}}
       <div class="col-6">
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="floatingInput" placeholder="id" value="{{this.id}}" name="id" readonly>
   
        </div>
    </div>

    <div class="col-6">
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="floatingInput" placeholder="first_name" value="{{this.first_name}}" readonly name="first_name">
   
        </div>
    </div>

        <div class="col-6">
        <div class="form-floating mb-3">
            <input type="text" class="form-control" id="floatingInput" placeholder="last_name" name="last_name" readonly value="{{this.last_name}}">
   
        </div>
    </div>

        <div class="col-6">
        <div class="form-floating mb-3">
            <input type="email" class="form-control" id="floatingInput" placeholder="Email@email.com"  name="email" readonly value="{{this.email}}">
   
        </div>
    </div>
    <div class="col-6">
        <div class="form-floating mb-3">
            <input type="tel" class="form-control" id="floatingInput" placeholder="phone" name="phone" value="{{phone}}" readonly>
   
        </div>
    </div>

    <div class="col-12">
        <div class="form-floating mb-3">
            <textarea class="form-control" id="floatingInput" placeholder="Comments" style="height: 200px;" name="comments" readonly>
                {{this.comments}}
            </textarea>
        </div>
    </div>
    <div class="col-12 d-grid" >
            <button type="Submit" style="width: 100%;" class="btn btn-primary">back</button>
    </div>


    {{/each}}
</form>